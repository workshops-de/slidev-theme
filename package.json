{
  "name": "@workshops.de/slidev-theme",
  "version": "2.1.0",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev",
    "workshops.de"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "slidev example.md --open",
    "build": "slidev build example.md",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "devDependencies": {
    "@slidev/cli": "52.11.4",
    "@slidev/types": "52.11.4",
    "prettier": "3.8.1",
    "prettier-plugin-slidev": "1.0.5"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "both"
  }
}
